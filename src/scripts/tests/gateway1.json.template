{
  "device_name":"web3.buckyos.io",
  "device_key_path":"/opt/cyfs_gateway/device_key.pem",
  "inner_services":{
    "main_sn" : {
      "type" : "cyfs-sn",
      "host":"web3.buckyos.io",
      "ip":"192.168.1.188",
      "zone_config_jwt":"TODO",
      "zone_config_pkx":"TODO"
    },
    "zone_provider" : {
      "type" : "zone-provider"
    }
  },
  "servers":{
    "main_http_server":{
      "type":"cyfs-warp",
      "bind":"0.0.0.0",
      "http_port":80,
      "tls_port":443,
      "hosts": {
        "web3.buckyos.ai": {
          "tls": {
            "disable_tls": false,
            "cert_path": "fullchain.cer",
            "key_path": "buckyos.ai.key",
            "enable_acme": false
          },
          "enable_cors":true,
          "routes": {
            "/kapi/sn":{
              "inner_service":"main_sn"
            },
            "/static":{
              "local_dir":"/opt/cyfs_gateway"
            },
            "/test_upstream": {
              "upstream": "http://127.0.0.1:8000"
            },
	        "/test_upstream_permanent": {
	          "upstream": "http://127.0.0.1:8000 redirect permanent"
	        },
            "/test_upstream_temporary": {
              "upstream": "http://127.0.0.1:8000 redirect temporary"
            }
          }
        },
        "web3.buckyos.com": {
          "tls": {
            "disable_tls": false,
            "cert_path": "fullchain.cer",
            "key_path": "buckyos.ai.key",
            "enable_acme": false
          },
          "enable_cors":false,
          "routes": {
            "/static":{
              "local_dir":"/opt/cyfs_gateway"
            },
            "/test_upstream": {
              "enable_cors":true,
              "upstream": "http://127.0.0.1:8000"
            },
	        "/test_upstream_permanent": {
	          "upstream": "http://127.0.0.1:8000 redirect permanent"
	        },
            "/test_upstream_temporary": {
              "upstream": "http://127.0.0.1:8000 redirect temporary"
            }
          }
        },
        "sn.buckyos.ai": {
          "tls": {
            "disable_tls": false,
            "cert_path": "fullchain.cer",
            "key_path": "buckyos.ai.key",
            "enable_acme": false
          },
          "routes": {
            "/":{
              "tunnel_selector":"main_sn"
            }
          }
        },
        "*":{
          "routes": {
            "/static":{
              "local_dir":"/opt/cyfs_gateway"
            },
            "/test_upstream": {
              "upstream": "http://127.0.0.1:8000"
            },
            "/test_upstream_permanent": {
              "upstream": "http://127.0.0.1:8000 redirect permanent"
            },
            "/test_upstream_temporary": {
              "upstream": "http://127.0.0.1:8000 redirect temporary"
            }
          }
        }
      }
    },
    "main_dns_server":{
      "type":"cyfs-dns",
      "bind":"$${dns_ip}",
      "port": 53,
      "this_name":"buckyos.ai",
      "resolver_chain": [
        {
          "type": "SN",
          "server_id": "main_sn"
        },
        {
          "type": "dns",
          "cache": true
        }
      ],
      "fallback": ["223.5.5.5","6.6.6.6"]
    }
  },

  "dispatcher" : {
    "udp://0.0.0.0:53":{
      "type":"server",
      "id":"main_dns_server"
    },
    "tcp://0.0.0.0:8080":{
      "type":"forward",
	  "target":"tcp:///127.0.0.1:8000"
    },
    "tcp://0.0.0.0:443":{
      "type":"server",
      "id":"main_http_server"
    }
  },
  "includes": []
}
