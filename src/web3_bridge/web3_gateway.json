{
    "inner_services":{
        "main_sn" : {
            "type" : "cyfs_sn"
        }
    },
    "servers":{
        "main_http_server":{
            "type":"cyfs-warp",
            "bind":"0.0.0.0",
            "http_port":80,
            "tls_port":443,
            "hosts": {
                "web3.buckyos.io": {
                    "routes": {
                        "/static": {
                            "local_dir": "/tmp/"
                        },
                        "/kapi/sn":{
                            "inner_service":"main_sn"
                        }
                    }
                }
            }
        }
    },
    "dispatcher" : {
        "tcp://0.0.0.0:80":{
            "type":"server",
            "id":"main_http_server"
        },
        "tcp://0.0.0.0:443":{
            "type":"server",
            "id":"main_http_server"
        }
    }
}