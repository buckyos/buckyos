{
    "device_name":"web3.buckyos.io",
    "device_key_path":"/opt/web3_bridge/device_key.pem",
    "inner_services":{
        "main_sn" : {
            "type" : "cyfs-sn",
            "host":"web3.buckyos.io",
            "ip":"192.168.1.188",
            "zone_config_jwt":"TODO",
            "zone_config_pkx":"TODO"
        },
        "zone_provider" : {
            "type" : "zone-provider"
        }
    },
    "servers":{
        "main_http_server":{
            "type":"cyfs-warp",
            "bind":"0.0.0.0",
            "http_port":80,
            "tls_port":443,
            "default_tls_host":"*.web3.buckyos.io",
            "hosts": {
                "web3.buckyos.io": {
                    "tls": {
                        "cert_path": "fullchain.pem",
                        "key_path": "privkey.pem"
                    },
                    "enable_cors":true,
                    "routes": {
                        "/kapi/sn":{
                            "inner_service":"main_sn"
                        },
                        "/static":{
                            "local_dir":"/tmp"
                        }
                    }
                },
                "*.web3.buckyos.io": {
                    "tls": {
                        "cert_path": "fullchain.pem",
                        "key_path": "privkey.pem"
                    },
                    "routes": {
                        "/":{
                            "tunnel_selector":"main_sn"
                        }
                    }
                },
                "*":{
                    "routes": {
                        "/":{
                            "tunnel_selector":"main_sn"
                        },
                        "/resolve":{
                            "inner_service":"zone_provider"
                        }
                    }
                }
            }
        },
        "main_dns_server":{
            "type":"cyfs-dns",
            "bind":"0.0.0.0",
            "port":2053,
            "this_name":"buckyos.io",
            "resolver_chain": [
                {
                  "type": "SN",
                  "server_id": "main_sn"
                },
                {
                    "type": "dns",
                    "cache": true
                }
            ],
            "fallback": ["8.8.8.8","6.6.6.6"]
        }
    },
    
    "dispatcher" : {
        "udp://0.0.0.0:53":{
            "type":"server",
            "id":"main_dns_server"
        },
        "tcp://0.0.0.0:80":{
            "type":"server",
            "id":"main_http_server"
        },
        "tcp://0.0.0.0:443":{
            "type":"server",
            "id":"main_http_server"
        }
    }
}